});\r\n        }\r\n      },\r\n      editCompany(company) {\r\n      document.getElementById(\"name\").value = company.name\r\n      document.getElementById(\"cnpj\").value = company.cnpj\r\n      document.getElementById(\"cep\").value = company.cep\r\n       this.company = company\r\n      },\r\n      alterCompanies() {\r\n       this.supplier.name = document.getElementById(\"name\").value\r\n       this.supplier.cnpj = document.getElementById(\"cnpj\").value\r\n       this.supplier.cep = document.getElementById(\"cep\").value\r\n  \r\n       axios.put(`https://localhost:7237/api/suppliers/Update/${this.supplier.id}`, this.supplier).then(() =>\r\n       {\r\n        this.getSuppliers()\r\n        this.company = undefined\r\n        document.getElementById(\"name\").value = \"\"\r\n        document.getElementById(\"cnpj\").value = \"\"\r\n        document.getElementById(\"cep\").value = \"\"\r\n  \r\n       })\r\n  \r\n  \r\n      },\r\n    },\r\n    created() {\r\n      this.getSuppliers();\r\n    },\r\n    components: {\r\n      Message,\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  #supplier-table {\r\n    max-width: 1200px;\r\n    margin: 0 auto;\r\n  }\r\n  \r\n  #supplier-table-heading,\r\n  #supplier-table-rows,\r\n  .supplier-table-row {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n  }\r\n  \r\n  #supplier-table-heading {\r\n    font-weight: bold;\r\n    padding: 12px;\r\n    border-bottom: 3px solid #333;\r\n  }\r\n  \r\n  .supplier-table-row {\r\n    width: 100%;\r\n    padding: 120px;\r\n    border-bottom: 1px solid #ccc;\r\n  }\r\n  \r\n  #supplier-table-heading div,\r\n  .supplier-table-row div {\r\n    width: 13%;\r\n  }\r\n  \r\n  #supplier-table-heading .order-id,\r\n  .supplier-table-row .order-number {\r\n    width: 5%;\r\n  }\r\n  \r\n  .edit-btn {\r\n    background-color: #222;\r\n    color: #fcba03;\r\n    font-weight: bold;\r\n    border: 2px solid #222;\r\n    padding: 10px;\r\n    font-size: 16px;\r\n    margin: 0 auto;