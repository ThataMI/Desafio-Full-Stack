>\r\n    </div>\r\n  </div>\r\n  <div v-else>\r\n    <h2>Não há pedidos no momento!</h2>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"CompanyTable\",\r\n  data() {\r\n    return {\r\n      companies: [],\r\n    };\r\n  },\r\n  methods: {\r\n    getCompanies() {\r\n      axios\r\n        .get(\"https://localhost:7237/api/companies/Get\")\r\n        .then((res) => {\r\n          console.log(res);\r\n          this.companies = res.data;\r\n        })\r\n        .catch(function (error) {\r\n          console.error(error);\r\n        });\r\n    },\r\n    updateCompanies(id) {\r\n      axios\r\n        .put(\"https://localhost:7237/api/companies/${id}\")\r\n        .then((res) => {\r\n          console.log(res);\r\n          this.companies = res.data;\r\n        })\r\n        .catch(function (error) {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n  created() {\r\n    this.getCompanies();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#company-table {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\n#company-table-heading,\r\n#company-table-rows,\r\n.company-table-row {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n#company-table-heading {\r\n  font-weight: bold;\r\n  padding: 12px;\r\n  border-bottom: 3px solid #333;\r\n}\r\n\r\n.company-table-row {\r\n  width: 100%;\r\n  padding: 12px;\r\n  border-bottom: 1px solid #ccc;\r\n}\r\n\r\n#company-table-heading div,\r\n.company-table-row div {\r\n  width: 19%;\r\n}\r\n\r\n#company-table-heading .order-id,\r\n.company-table-row .order-number {\r\n  width: 5%;\r\n}\r\n\r\nselect {\r\n  padding: 12px 6px;\r\n  margin-right: 12px;\r\n}\r\n\r\n.delete-btn {\r\n  background-color: #222;\r\n  color: #fcba03;\r\n  font-weight: bold;\r\n  border: 2px solid #222;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  margin: 0 auto;\r\n  cursor: pointer;\r\n  transition: 0.5s;\r\n}\r\n\r\n.delete-btn:hover {\r\n  background-color: transp