Year() \r\n      axios\r\n        .post(\"https://localhost:7237/api/suppliers/Post\", this.form)\r\n        .then(() => {\r\n          this.msg = \"Cadastro realizado\";\r\n          setTimeout(() => (this.msg = \"\"), 3000);\r\n          console.log(bDay)\r\n        })\r\n        .catch(function (error) {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n\r\n  components: {\r\n    Message,\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#supplier-form {\r\n  max-width: 200px;\r\n  margin: 0 auto;\r\n  margin-left: 300px;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.input-container-supplier {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-between;\r\n  margin-bottom: 20px;\r\n}\r\n\r\nlabel {\r\n  font-weight: bold;\r\n  margin-bottom: 15px;\r\n  color: #222;\r\n  padding: 5px 10px;\r\n  border-left: 4px solid #987bad;\r\n}\r\n\r\ninput {\r\n  padding: 5px 10px;\r\n  width: 300px;\r\n  border-radius: .5rem;\r\n  font-size: 1rem;\r\n  margin-top: 7px;\r\n  border-color: #987bad;\r\n}\r\n\r\n.submit-btn {\r\n  background-color: #222;\r\n  color: #987bad;\r\n  font-weight: bold;\r\n  border: 2px solid #222;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  margin: 0 auto;\r\n  cursor: pointer;\r\n  transition: 0.5s;\r\n}\r\n\r\n.submit-btn:hover {\r\n  background-color: transparent;\r\n  color: #222;\r\n}\r\n</style>\r\n"],"mappings":"AA0EA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,WAAW;AAE/B,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,IAAI,EAAE;QACJH,IAAI,EAAE,EAAE;QACRI,OAAO,EAAE,EAAE;QACXC,GAAG,EAAE,EAAE;QACPC,KAAK,EAAE,EAAE;QACTC,EAAE,EAAE,EAAE;QACNC,EAAE,EAAE,EAAE;QACNC,QAAQ,EAAE;MACZ,CAAC;MACDA,QAAQ,EAAE,IAAI;MACdC,GAAG,EAAE,EAAE;MACPC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAAS,EAAE,eAAAA,CAAA,EAAiB;MAC1B,IAAG,IAAI,CAACV,IAAI,CAACE,GAAG,CAACS,MAAK,IAAK,CAAC,EAAE;QAChChB,KAAK,CAACiB,GAAG,CAAE,4BAA4B,IAAI,CAACZ,IAAI,CAACE,GAAK,QAAO,EAC5DW,IAAI,CAAEL,QAAO,IAAK,IAAI,C