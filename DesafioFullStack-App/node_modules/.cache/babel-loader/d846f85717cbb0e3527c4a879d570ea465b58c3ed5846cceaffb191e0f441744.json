{"ast":null,"code":"import axios from \"axios\";\nimport Message from \"./Message\";\nexport default {\n  name: \"SupplierForm\",\n  data() {\n    return {\n      suppliers: [],\n      form: {\n        name: '',\n        cnpjCpf: '',\n        cep: '',\n        email: '',\n        uf: '',\n        rg: '',\n        birthday: null\n      },\n      msg: '',\n      response: null\n    };\n  },\n  methods: {\n    getAge(bDate) {\n      const today = new Date();\n      const birthDate = new Date(bDate);\n      let age = today.getFullYear() - birthDate.getFullYear();\n      const m = today.getMonth() - birthDate.getMonth();\n      if (m < 0 || m === 0 && today.getDate() < birthDate.getDate()) {\n        age--;\n      }\n      return age;\n    },\n    checkForm: async function () {\n      if (this.form.cep.length == 8) {\n        axios.get(`https://viacep.com.br/ws/${this.form.cep}/json/`).then(response => this.data = response.data).catch(error => console.log(error));\n      } else alert(\"CEP Invalido!\");\n    },\n    checkUf: async function () {\n      if (this.form.uf == \"PR\" && this.form.cnpjCpf.length == 11) {\n        axios.get(`https://viacep.com.br/ws/${this.form.cep}/json/`).then(response => this.data = response.data).catch(error => console.log(error));\n      } else alert(\"CEP Invalido!\");\n    },\n    onSubmit() {\n      if (this.form.uf == \"PR\" && this.form.cnpjCpf.length == 11) {\n        const age = this.getAge(Date.parse(this.form.birthday));\n        if (age < 18) {}\n        return;\n      }\n      const currentDate = new Date();\n      const bDay = this.getAge(Date.parse(this.form.birthday));\n      axios.post(\"https://localhost:7237/api/suppliers/Post\", this.form).then(() => {\n        this.msg = \"Cadastro realizado\";\n        setTimeout(() => this.msg = \"\", 3000);\n        console.log(bDay);\n      }).catch(function (error) {\n        console.error(error);\n      });\n    }\n  },\n  components: {\n    Message\n  }\n};","map":{"version":3,"nam