onents\\SupplierTable.vue"],"sourcesContent":["<template>\r\n    <div id=\"supplier-table\">\r\n      <Message :msg=\"msg\" v-show=\"msg\" />\r\n      <div><input v-model=\"SearchName\" placeholder=\"Pesquisar nome:\"></div>\r\n      <div> <input v-model=\"SearchCnpjCpf\" placeholder=\"Pesquisar cnpj/cpf:\"></div>\r\n     \r\n      <div id=\"supplier-table-heading\">\r\n        <div>Nome:</div>\r\n        <div>Cnpj/Cpf:</div>\r\n        <div>Cep:</div>\r\n        <div>E-mail:</div>\r\n        <div>Rg:</div>\r\n        <div>Data de Nascimento:</div>\r\n        <div>Deletar</div>\r\n      </div>\r\n      <div id=\"supplier-table-rows\">\r\n        <div\r\n          class=\"supplier-table-row\"\r\n          v-for=\"supplier in suppliers\"\r\n          :key=\"supplier.suppliersId\"\r\n        >\r\n          <div>\r\n            {{ supplier.name }}\r\n          </div>\r\n          <div>\r\n            {{ supplier.cnpjCpf }}\r\n          </div>\r\n          <div>\r\n            {{ supplier.cep }}\r\n          </div>\r\n          <div>\r\n            {{ supplier.email }}\r\n          </div>\r\n          <div>\r\n            {{ supplier.rg }}\r\n          </div>\r\n          <div>\r\n            {{ supplier.birthday }}\r\n          </div>\r\n          <div>\r\n            <!-- <button class=\"edit-btn\" @click=\"editCompany(supplier)\">\r\n              Editar\r\n            </button> -->\r\n            <button\r\n              class=\"delete-btn\"\r\n              @click=\"deleteCompany(supplier.suppliersId)\"\r\n            >\r\n              Deletar\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from \"axios\";\r\n  import Message from \"./Message\";\r\n\r\n  let searchName\r\n  let SearchCnpjCpf\r\n  \r\n  export default {\r\n    name: \"SupplierTable\",\r\n  \r\n    data() {\r\n      return {\r\n        suppliers: [],\r\n        msg: \"\",\r\n      };\r\n    },\r\n    m