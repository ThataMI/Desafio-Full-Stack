\r\n        suppliers: [],\r\n        supplierFilter: [],\r\n        msg: \"\",\r\n      };\r\n    },\r\n   const: supplierFilter = computed(() => {\r\n    \r\n      let suppliers = [];\r\n      suppliers = this.suppliers.filter((supplier) => {\r\n        return (\r\n            supplier.name.toLowerCase().indexOf(this.search.toLowerCase()) ||\r\n            supplier.cnpjCpf.toLowerCase().indexOf(this.search.toLowerCase())\r\n        );\r\n      });\r\n\r\n      return suppliers;\r\n    \r\n  }),\r\n    methods: {\r\n      getSuppliers() {\r\n        axios\r\n          .get(\"https://localhost:7237/api/suppliers/GetAll\")\r\n          .then((res) => {\r\n            this.suppliers = res.data;\r\n            this.supplierFilter = res.data;\r\n          })\r\n          .catch(function (error) {\r\n            console.error(error);\r\n          });\r\n      },\r\n      getSuppliersName () {\r\n        if(this.SearchName.length > 0){\r\n          this.supplierFilter = this.suppliers.filter(supplier => supplier.name.toLowerCase().includes(this.SearchName.toLowerCase()));\r\n        }else{\r\n          this.supplierFilter = this.suppliers;\r\n        }\r\n      },\r\n      getSuppliersCnpjCpf () {\r\n        if(this.SearchCnpjCpf.length > 0){\r\n          this.supplierFilter = this.suppliers.filter(supplier => supplier.cnpjCpf.toLowerCase().includes(this.SearchCnpjCpf.toLowerCase()));\r\n        }else{\r\n          this.supplierFilter = this.suppliers;\r\n        }\r\n      },\r\n      deleteCompany(id) {\r\n        if (confirm(\"Desaja deletar essa empresa?\")) {\r\n          axios\r\n            .delete(`https://localhost:7237/api/suppliers/Delete/${id}`)\r\n            .then((res) => {\r\n              this.suppliers = res.data;\r\n              this.supplierFilter = res.data;\r\n              this.getSuppliers();\r\n              this.msg = \"Empresa Deletada\";\r\n              setTimeout(() => (this.msg = \"\"), 2000);\r\n            })\r\n            .catch((error) => {\r\n              console.log(error);\r\n            });\r\n        }\r\n      },\r\n    },\r\n    created() {\r\n      this.getSuppliers();\r\n    },\r\n    components: {\r\n      Message,\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  input {\r\n  padding: 5px 10px;\r\n  width: 300px;\r\n  border-radius: 10px;\r\n  font-size: 16px;\r\n  margin-top: 10px;\r\n  margin-bottom: 10px;\r\n  margin-left: 10px;\r\n  border-color: #987bad;\r\n}\r\n  .delete-btn {\r\n  background-color: #222;\r\n  color           : #987bad;\r\n  font-weight     : bold;\r\n  border          : 2px solid #222;\r\n  padding         : 10px;\r\n  font-size       : 16px;\r\n  margin          : 0 auto;\r\n  cursor          : pointer;\r\n  transition      : 0.5s;\r\n  }\r\n  \r\n  .delete-btn:hover {\r\n    background-color: transparent;\r\n    color: #222;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAwCE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,WAAW;AAC/B,OAAQC,QAAO,MAAQ,KAAK;AAE5B,eAAe;EACbC,IAAI,EAAE,eAAe;EAErBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,cAAc,EAAE,EAAE;MAClBC,GAAG,EAAE;IACP,CAAC;EACH,CAAC;EACFC,KAAK,EAAEF,cAAa,GAAIJ,QAAQ,CAAC,MAAM;IAEpC,IAAIG,SAAQ,GAAI,EAAE;IAClBA,SAAQ,GAAI,IAAI,CAACA,SAAS,CAACI,MAAM,CAAEC,QAAQ,IAAK;MAC9C,OACIA,QAAQ,CAACP,IAAI,CAACQ,WAAW,CAAC,CAAC,CAACC,OAAO,CAAC,IAAI,CAACC,MAAM,CAACF,WAAW,CAAC,CAAC,KAC7DD,QAAQ,CAACI,OAAO,CAACH,WAAW,CAAC,CAAC,CAACC,OAAO,CAAC,IAAI,CAACC,MAAM,CAACF,WAAW,CAAC,CAAC;IAEtE,CAAC,CAAC;IAEF,OAAON,SAAS;EAEpB,CAAC,CAAC;EACAU,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACbhB,KAAI,CACDiB,GAAG,CAAC,6CAA6C,EACjDC,IAAI,CAAEC,GAAG,IAAK;QACb,IAAI,CAACd,SAAQ,GAAIc,GAAG,CAACf,IAAI;QACzB,IAAI,CAACE,cAAa,GAAIa,GAAG,CAACf,IAAI;MAChC,CAAC,EACAgB,KAAK,CAAC,UAAUC,KAAK,EAAE;QACtBC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDE,gBAAeA,CAAA,EAAK;MAClB,IAAG,IAAI,CAACC,UAAU,CAACC,MAAK,GAAI,CAAC,EAAC;QAC5B,IAAI,CAACnB,cAAa,GAAI,IAAI,CAACD,SAAS,CAACI,MAAM,CAACC,QAAO,IAAKA,QAAQ,CAACP,IAAI,CAACQ,WAAW,CAAC,CAAC,CAACe,QAAQ,CAAC,