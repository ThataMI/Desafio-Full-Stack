./Message\";\r\n\r\nexport default {\r\n  name: \"CompanyTable\",\r\n\r\n  data() {\r\n    return {\r\n      companies: [],\r\n      company: undefined,\r\n      msg: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    getCompanies() {\r\n      axios\r\n        .get(\"https://localhost:7237/api/companies/Get\")\r\n        .then((res) => {\r\n          console.log(res);\r\n          this.companies = res.data;\r\n        })\r\n        .catch(function (error) {\r\n          console.error(error);\r\n        });\r\n    },\r\n    deleteCompany(id) {\r\n      if (confirm(\"Desaja deletar essa empresa?\")) {\r\n        axios\r\n          .delete(`https://localhost:7237/api/companies/Delete/${id}`)\r\n          .then((res) => {\r\n            this.companies = res.data;\r\n            this.getCompanies();\r\n            this.msg = \"Empresa Deletada\";\r\n            setTimeout(() => (this.msg = \"\"), 2000);\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          });\r\n      }\r\n    },\r\n    editCompany(company) {\r\n    document.getElementById(\"name\").value = company.name\r\n    document.getElementById(\"cnpj\").value = company.cnpj\r\n    document.getElementById(\"cep\").value = company.cep\r\n     this.company = company\r\n    },\r\n    alterCompanies() {\r\n     this.company.name = document.getElementById(\"name\").value\r\n     this.company.cnpj = document.getElementById(\"cnpj\").value\r\n     this.company.cep = document.getElementById(\"cep\").value\r\n\r\n     axios.put(`https://localhost:7237/api/companies/Update/${this.company.id}`, this.company).then(() =>\r\n     {\r\n      this.getCompanies()\r\n      this.company = undefined\r\n      document.getElementById(\"name\").value = \"\"\r\n      document.getElementById(\"cnpj\").value = \"\"\r\n      document.getElementById(\"cep\").value = \"\"\r\n\r\n     })\r\n\r\n\r\n    },\r\n  },\r\n  created() {\r\n    this.getCompanies();\r\n  },\r\n  components: {\r\n    Message,\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#company-table {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  align-items: center;\r\n}\r\n\r\n#company-table-heading,\r\n#company-table-rows,\r\n.company-table-row {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n#company-table-heading {\r\n  font-weight: bold;\r\n  padding: 12px;\r\n  border-bottom: 3px solid #333;\r\n}\r\n\r\n.company-table-row {\r\n  width: 100%;\r\n  padding: 12px;\r\n  border-bottom: 1px solid #ccc;\r\n}\r\n\r\n#company-table-heading div,\r\n.company-table-row div {\r\n  width: 19%;\r\n}\r\n\r\n#company-table-heading .order-id,\r\n.company-table-row .order-number {\r\n  width: 5%;\r\n}\r\n\r\n.edit-btn {\r\n  background-color: #222;\r\n  color: #fcba03;\r\n  font-weight: bold;\r\n  border: 2px solid #222;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  margin: 0 auto;\r\n  cursor: pointer;\r\n  transition: 0.5s;\r\n}\r\n\r\n.edit-btn:hover {\r\n  background-color: transparent;\r\n  color: #222;\r\n}\r\n.delete-btn {\r\n  background-color: #222;\r\n  color: #fcba03;\r\n  font-weight: bold;\r\n  border: 2px solid #222;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  margin: 0 auto;\r\n  cursor: pointer;\r\n  transition: 0.5s;\r\n}\r\n\r\n.delete-btn:hover {\r\n  background-color: transparent;\r\n  color: #222;\r\n}\r\n</style>\r\n"],"mappings":";;;EACOA,EAAE,EAAC;AAAe;gEAErBC,mBAAA,CAKM;EALDD,EAAE,EAAC;AAAuB,I,aAC7BC,mBAAA,CAAgB,aAAX,OAAK,G,aACVA,mBAAA,CAAgB,aAAX,OAAK,G,aACVA,mBAAA,CAAe,aAAV,MAAI,G,aACTA,mBAAA,CAAgB,aAAX,OAAK,E;;EAEPD,EAAE,EAAC;AAAoB;;;;;uBAR9BE,mBAAA,CAoCM,OApCNC,UAoCM,G,gBAnCJC,YAAA,CAAmCC,kBAAA;IAAzBC,GAAG,EAAEC,KAAA,CAAAD;EAAG,mC,SAAUC,KAAA,CAAAD,GAAG,E,GAC/BE,UAKM,EACNP,mBAAA,CA2BM,OA3BNQ,UA2BM,I,kBA1BJP,mBAAA,CAyBMQ,SAAA,QAAAC,WAAA,CAvBcJ,KAAA,CAAAK,SAAS,EAApBC,OAAO;yBAFhBX,mBAAA,CAyBM;MAxBJY,KAAK,EAAC,mBAAmB;MAExBC,GAAG,EAAEF,OAAO,CAACG;QAEdf,mBAAA,CAEM,aAAAgB,gBAAA,CADDJ,OAAO,CAACK,IAAI,kBAEjBjB,mBAAA,CAEM,aAAAgB,gBAAA,CADDJ,OAAO,CAACM,IAAI,kBAEjBlB,mBAAA,CAEM,aAAA