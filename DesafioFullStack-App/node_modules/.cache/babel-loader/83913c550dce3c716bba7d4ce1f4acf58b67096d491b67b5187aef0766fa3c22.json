{"ast":null,"code":"import axios from \"axios\";\nimport Message from \"./Message\";\nexport default {\n  name: \"SupplierTable\",\n  data() {\n    return {\n      suppliers: [],\n      msg: \"\"\n    };\n  },\n  methods: {\n    checkName: function () {\n      if (this.FilterName.length > 0) {\n        this.supliersFilter = this.supliers.filter(c => c.name.includes(this.FilterName));\n      } else {\n        this.supliersFilter = this.supliers;\n      }\n    },\n    checkCnpj: function () {\n      if (this.FilterCnpj.length > 10) {\n        this.supliersFilter = this.supliers.filter(c => c.cnpjCpf === this.FilterCnpj);\n      } else {\n        this.supliersFilter = this.supliers;\n      }\n    },\n    getSuppliers() {\n      axios.get(\"https://localhost:7237/api/suppliers/GetAll\").then(res => {\n        this.suppliers = res.data;\n      }).catch(function (error) {\n        console.error(error);\n      });\n    },\n    getSuppliersByName(name) {\n      axios.get(`https://localhost:7237/api/suppliers/Name/${name}`).then(res => {\n        this.suppliers = res.data;\n      }).catch(function (error) {\n        console.error(error);\n      });\n    },\n    deleteCompany(id) {\n      if (confirm(\"Desaja deletar essa empresa?\")) {\n        axios.delete(`https://localhost:7237/api/suppliers/Delete/${id}`).then(res => {\n          this.suppliers = res.data;\n          this.getSuppliers();\n          this.msg = \"Empresa Deletada\";\n          setTimeout(() => this.msg = \"\", 2000);\n        }).catch(error => {\n          console.log(error);\n        });\n      }\n    },\n    editCompany(company) {\n      document.getElementById(\"name\").value = company.name;\n      document.getElementById(\"cnpj\").value = company.cnpj;\n      document.getElementById(\"cep\").value = company.cep;\n      this.company = company;\n    },\n    alterCompanies() {\n      this.supplier.name = document.getElementById(\"name\").value;\n      this.supplier.cnpj = document.getElementById(\"cnpj\").value;\n      this.supplier.cep = document.getElementById(\"cep\").value;\n      axios.put(`https://localhost:7237/api/suppliers/Update/${this.supplier.id}`, this.supplier).then(() => {\n        this.getSuppliers();\n        this.company = undefined;\n        document.getElementById(\"name\").value = \"\";\n        document.getElementById(\"cnpj\").value = \"\";\n        document.getElementById(\"cep\").value = \"\";\n      });\n    }\n  },\n  created() {\n    this.getSuppliers();\n  },\n  components: {\n    Message\n  }\n};","map":{"version":3,"names":["axios","Message","name","data","suppliers","msg","methods","checkName","FilterName","length","supliersFilter","supliers","filter","c","includes","checkCnpj","FilterCnpj","cnpjCpf","getSuppliers","get","then","res","catch","error","console","getSuppliersByName","deleteCompany","id","confirm","delete","setTimeout","log","editCompany","company","document","getElementById","value","cnpj","cep","alterCompanies","supplier","put","undefined","created","components"],"sources":["C:\\Users\\Thata\\OneDrive\\√Årea de Trabalho\\DesafioAccenture\\desafiofullstack-app\\src\\components\\SupplierTable.vue"],"sourcesContent":["<template>\r\n    <div id=\"supplier-table\">\r\n      <Message :msg=\"msg\" v-show=\"msg\" />\r\n      <input v-model=\"SearchName\" placeholder=\"Pesquisar nome:\">\r\n      <input v-model=\"SearchCnpjCpf\" placeholder=\"Pesquisar cnpj/cpf:\">\r\n      <div id=\"supplier-table-heading\">\r\n        <div>Nome:</div>\r\n        <div>Cnpj/Cpf:</div>\r\n        <div>Cep:</div>\r\n        <div>E-mail:</div>\r\n        <div>Rg:</div>\r\n        <div>Data de Nascimento:</div>\r\n        <div>Deletar</div>\r\n      </div>\r\n      <div id=\"supplier-table-rows\">\r\n        <div\r\n          class=\"supplier-table-row\"\r\n          v-for=\"supplier in suppliers\"\r\n          :key=\"supplier.suppliersId\"\r\n        >\r\n          <div>\r\n            {{ supplier.name }}\r\n          </div>\r\n          <div>\r\n            {{ supplier.cnpjCpf }}\r\n          </div>\r\n          <div>\r\n            {{ supplier.cep }}\r\n          </div>\r\n          <div>\r\n            {{ supplier.email }}\r\n          </div>\r\n          <div>\r\n            {{ supplier.rg }}\r\n          </div>\r\n          <div>\r\n            {{ supplier.birthday }}\r\n          </div>\r\n          <div>\r\n            <!-- <button class=\"edit-btn\" @click=\"editCompany(supplier)\">\r\n              Editar\r\n            </button> -->\r\n            <button\r\n              class=\"delete-btn\"\r\n              @click=\"deleteCompany(supplier.suppliersId)\"\r\n            >\r\n              Deletar\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from \"axios\";\r\n  import Message from \"./Message\";\r\n  \r\n  export default {\r\n    name: \"SupplierTable\",\r\n  \r\n    data() {\r\n      return {\r\n        suppliers: [],\r\n        msg: \"\",\r\n      };\r\n    },\r\n    methods: {\r\n        checkName: function () {\r\n        if(this.FilterName.length > 0){\r\n          this.supliersFilter = this.supliers.filter(c => c.name.includes(this.FilterName));\r\n        }else{\r\n          this.supliersFilter = this.supliers;\r\n        }\r\n      },\r\n      checkCnpj: function () {\r\n        if(this.FilterCnpj.length > 10){\r\n          this.supliersFilter = this.supliers.filter(c => c.cnpjCpf === this.FilterCnpj);\r\n        }else{\r\n          this.supliersFilter = this.supliers;\r\n        }\r\n      },\r\n      getSuppliers() {\r\n        axios\r\n          .get(\"https://localhost:7237/api/suppliers/GetAll\")\r\n          .then((res) => {\r\n           this.suppliers = res.data;\r\n          })\r\n          .catch(function (error) {\r\n            console.error(error);\r\n          });\r\n      },\r\n      getSuppliersByName(name) {\r\n        axios\r\n          .get(`https://localhost:7237/api/suppliers/Name/${name}`)\r\n          .then((res) => {\r\n           this.suppliers = res.data;\r\n          })\r\n          .catch(function (error) {\r\n            console.error(error);\r\n          });\r\n      },\r\n      deleteCompany(id) {\r\n        if (confirm(\"Desaja deletar essa empresa?\")) {\r\n          axios\r\n            .delete(`https://localhost:7237/api/suppliers/Delete/${id}`)\r\n            .then((res) => {\r\n              this.suppliers = res.data;\r\n              this.getSuppliers();\r\n              this.msg = \"Empresa Deletada\";\r\n              setTimeout(() => (this.msg = \"\"), 2000);\r\n            })\r\n            .catch((error) => {\r\n              console.log(error);\r\n            });\r\n        }\r\n      },\r\n      editCompany(company) {\r\n      document.getElementById(\"name\").value = company.name\r\n      document.getElementById(\"cnpj\").value = company.cnpj\r\n      document.getElementById(\"cep\").value = company.cep\r\n       this.company = company\r\n      },\r\n      alterCompanies() {\r\n       this.supplier.name = document.getElementById(\"name\").value\r\n       this.supplier.cnpj = document.getElementById(\"cnpj\").value\r\n       this.supplier.cep = document.getElementById(\"cep\").value\r\n  \r\n       axios.put(`https://localhost:7237/api/suppliers/Update/${this.supplier.id}`, this.supplier).then(() =>\r\n       {\r\n        this.getSuppliers()\r\n        this.company = undefined\r\n        document.getElementById(\"name\").value = \"\"\r\n        document.getElementById(\"cnpj\").value = \"\"\r\n        document.getElementById(\"cep\").value = \"\"\r\n  \r\n       })\r\n  \r\n  \r\n      },\r\n    },\r\n    created() {\r\n      this.getSuppliers();\r\n    },\r\n    components: {\r\n      Message,\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  #supplier-table {\r\n    max-width: 1200px;\r\n    margin: 0 auto;\r\n  }\r\n  \r\n  #supplier-table-heading,\r\n  #supplier-table-rows,\r\n  .supplier-table-row {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n  }\r\n  \r\n  #supplier-table-heading {\r\n    font-weight: bold;\r\n    padding: 12px;\r\n    border-bottom: 3px solid #333;\r\n  }\r\n  \r\n  .supplier-table-row {\r\n    width: 100%;\r\n    padding: 12px;\r\n    border-bottom: 1px solid #ccc;\r\n  }\r\n  \r\n  #supplier-table-heading div,\r\n  .supplier-table-row div {\r\n    width: 10%;\r\n  }\r\n  \r\n  #supplier-table-heading .order-id,\r\n  .supplier-table-row .order-number {\r\n    width: 5%;\r\n  }\r\n  \r\n  .edit-btn {\r\n    background-color: #222;\r\n    color: #fcba03;\r\n    font-weight: bold;\r\n    border: 2px solid #222;\r\n    padding: 10px;\r\n    font-size: 16px;\r\n    margin: 0 auto;\r\n    cursor: pointer;\r\n    transition: 0.5s;\r\n  }\r\n  \r\n  .edit-btn:hover {\r\n    background-color: transparent;\r\n    color: #222;\r\n  }\r\n  .delete-btn {\r\n    background-color: #222;\r\n    color: #fcba03;\r\n    font-weight: bold;\r\n    border: 2px solid #222;\r\n    padding: 10px;\r\n    font-size: 16px;\r\n    margin: 0 auto;\r\n    cursor: pointer;\r\n    transition: 0.5s;\r\n  }\r\n  \r\n  .delete-btn:hover {\r\n    background-color: transparent;\r\n    color: #222;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAuDE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,WAAW;AAE/B,eAAe;EACbC,IAAI,EAAE,eAAe;EAErBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,GAAG,EAAE;IACP,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACLC,SAAS,EAAE,SAAAA,CAAA,EAAY;MACvB,IAAG,IAAI,CAACC,UAAU,CAACC,MAAK,GAAI,CAAC,EAAC;QAC5B,IAAI,CAACC,cAAa,GAAI,IAAI,CAACC,QAAQ,CAACC,MAAM,CAACC,CAAA,IAAKA,CAAC,CAACX,IAAI,CAACY,QAAQ,CAAC,IAAI,CAACN,UAAU,CAAC,CAAC;MACnF,CAAC,MAAI;QACH,IAAI,CAACE,cAAa,GAAI,IAAI,CAACC,QAAQ;MACrC;IACF,CAAC;IACDI,SAAS,EAAE,SAAAA,CAAA,EAAY;MACrB,IAAG,IAAI,CAACC,UAAU,CAACP,MAAK,GAAI,EAAE,EAAC;QAC7B,IAAI,CAACC,cAAa,GAAI,IAAI,CAACC,QAAQ,CAACC,MAAM,CAACC,CAAA,IAAKA,CAAC,CAACI,OAAM,KAAM,IAAI,CAACD,UAAU,CAAC;MAChF,CAAC,MAAI;QACH,IAAI,CAACN,cAAa,GAAI,IAAI,CAACC,QAAQ;MA