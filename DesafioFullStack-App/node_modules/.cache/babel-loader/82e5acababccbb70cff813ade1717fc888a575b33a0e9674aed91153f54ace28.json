}}</td>\r\n      <td>  <button\r\n              class=\"delete-btn\"\r\n              @click=\"deleteCompany(supplier.suppliersId)\"\r\n            >\r\n              Deletar\r\n            </button></td>\r\n    </tr>\r\n    \r\n  </tbody>\r\n</table>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from \"axios\";\r\n  import Message from \"./Message\";\r\n  \r\n  export default {\r\n    name: \"SupplierTable\",\r\n  \r\n    data() {\r\n      return {\r\n        suppliers: [],\r\n        supplierFilter: [],\r\n        msg: \"\",\r\n      };\r\n    },\r\n    computed: {\r\n    filteredItems() {\r\n      let items = [];\r\n      items = this.items.filter((item) => {\r\n        return (\r\n          item.name.toLowerCase().indexOf(this.search.toLowerCase()) > -1 ||\r\n          item.cnpj.toLowerCase().indexOf(this.search.toLowerCase()) > -1\r\n        );\r\n      });\r\n\r\n      items = items.filter((item) => {\r\n        if (this.selected == null) return item;\r\n        return item.isActive === this.selected;\r\n      });\r\n\r\n      return items;\r\n    },\r\n  },\r\n    methods: {\r\n      getSuppliers() {\r\n        axios\r\n          .get(\"https://localhost:7237/api/suppliers/GetAll\")\r\n          .then((res) => {\r\n            this.suppliers = res.data;\r\n            this.supplierFilter = res.data;\r\n          })\r\n          .catch(function (error) {\r\n            console.error(error);\r\n          });\r\n      },\r\n      deleteCompany(id) {\r\n        if (confirm(\"Desaja deletar essa empresa?\")) {\r\n          axios\r\n            .delete(`https://localhost:7237/api/suppliers/Delete/${id}`)\r\n            .then((res) => {\r\n              this.suppliers = res.data;\r\n              this.getSuppliers();\r\n              this.msg = \"Empresa Deletada\";\r\n              setTimeout(() => (this.msg = \"\"), 2000);\r\n            })\r\n            .catch((error) => {\r\n              console.log(error);\r\n            });\r\n        }\r\n      },\r\n      editCompany(company) {\r\n      document.getElementById(\"name\").value = company.name\r\n      document.getElementById(\"cnpj\").value = company.cnpj\r\n      document.getElementById(\"cep\").value = company.cep\r\n       this.company = company\r\n      },\r\n      alterCompanies() {\r\n       this.supplier.name = document.getElementById(\"name\").value\r\n       this.supplier.cnpj = document.getElementById(\"cnpj\").value\r\n       this.supplier.cep = document.getElementById(\"cep\").value\r\n  \r\n       axios.put(`https://localhost:7237/api/suppliers/Update/${this.supplier.id}`, this.supplier).then(() =>\r\n       {\r\n        this.getSuppliers()\r\n        this.company = undefined\r\n        document.getElementById(\"name\").value = \"\"\r\n        document.getElementById(\"cnpj\").value = \"\"\r\n        document.getElementById(\"cep\").value = \"\"\r\n  \r\n       })\r\n  \r\n  \r\n      },\r\n    },\r\n    created() {\r\n      this.getSuppliers();\r\n    },\r\n    components: {\r\n      Message,\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .delete-btn {\r\n    background-color: #222;\r\n    color: #fcba03;\r\n    font-weight: bold;\r\n    border: 2px solid #222;\r\n    padding: 10px;\r\n    font-size: 16px;\r\n    margin: 0 auto;\r\n    cursor: pointer;\r\n    transition: 0.5s;\r\n  }\r\n  \r\n  .delete-btn:hover {\r\n    background-color: transparent;\r\n    color: #222;\r\n  }\r\n  </style>\r\n  "],"mappings":";;;EAGaA,KAAK,EAAC;AAAO;gEACxBC,mBAAA,CAUQ,gB,aATNA,mBAAA,CAQK,a,aAPHA,mBAAA,CAAyB;EAArBC,KAAK,EAAC;AAAK,GAAC,MAAI,G,aACpBD,mBAAA,CAA6B;EAAzBC,KAAK,EAAC;AAAK,GAAC,UAAQ,G,aACxBD,mBAAA,CAAwB;EAApBC,KAAK,EAAC;AAAK,GAAC,KAAG,G,aACnBD,mBAAA,CAA2B;EAAvBC,KAAK,EAAC;AAAK,GAAC,QAAM,G,aACtBD,mBAAA,CAAuB;EAAnBC,KAAK,EAAC;AAAK,GAAC,IAAE,G,aAClBD,mBAAA,CAAuC;EAAnCC,KAAK,EAAC;AAAK,GAAC,oBAAkB,G,aAClCD,mBAAA,CAAyB;EAArBC,KAAK,EAAC;AAAK,GAAC,MAAI,E;;;;uBAXtBC,mBAAA,CAiCM,c,gBAhCJC,YAAA,CAAmCC,kBAAA;IAAzBC,GAAG,EAAEC,KAAA,CAA