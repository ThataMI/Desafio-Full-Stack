{"ast":null,"code":"import axios from \"axios\";\nimport Message from \"./Message\";\nexport default {\n  name: \"SupplierTable\",\n  data() {\n    return {\n      suppliers: [],\n      supplierFilter: [],\n      msg: \"\"\n    };\n  },\n  computed: {\n    filteredItems() {\n      let items = [];\n      items = this.items.filter(item => {\n        return item.name.toLowerCase().indexOf(this.search.toLowerCase()) > -1 || item.cnpj.toLowerCase().indexOf(this.search.toLowerCase()) > -1;\n      });\n      return items;\n    },\n    const: supplierFilter = computed(() => {\n      if (suppliers.value && SearchName.value) {\n        return suppliers.value.filter(supplier => supplier.name.toLowerCase().includes(SearchName.value.toLowerCase()));\n      }\n      return suppliers.value;\n    })\n  },\n  methods: {\n    getSuppliers() {\n      axios.get(\"https://localhost:7237/api/suppliers/GetAll\").then(res => {\n        this.suppliers = res.data;\n        this.supplierFilter = res.data;\n      }).catch(function (error) {\n        console.error(error);\n      });\n    },\n    deleteCompany(id) {\n      if (confirm(\"Desaja deletar essa empresa?\")) {\n        axios.delete(`https://localhost:7237/api/suppliers/Delete/${id}`).then(res => {\n          this.suppliers = res.data;\n          this.supplierFilter = res.data;\n          this.getSuppliers();\n          this.msg = \"Empresa Deletada\";\n          setTimeout(() => this.msg = \"\", 2000);\n        }).catch(error => {\n          console.log(error);\n        });\n      }\n    }\n  },\n  created() {\n    this.getSuppliers();\n  },\n  components: {\n    Message\n  }\n};","map":{"version":3,"names":["axios","Message","name","data","suppliers","supplierFilter","msg","computed","filteredItems","items","filter","item","toLowerCase","indexOf","search","cnpj","const","value","SearchName","supplier","includes","methods","getSuppliers","get","then","res","catch","error","console","deleteCompany","id","confirm","delete","setTimeout","log","created","components"],"sources":["C:\\Users\\Thata\\OneDrive\\Área de Trabalho\\DesafioAccenture\\desafiofullstack-app\\src\\components\\SupplierTable.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <Message :msg=\"msg\" v-show=\"msg\" />\r\n      <input v-model=\"SearchName\"  placeholder=\"Pesquisar nome\">\r\n      <table class=\"table\">\r\n  <thead>\r\n    <tr>\r\n      <th scope=\"col\">Nome</th>\r\n      <th scope=\"col\">Cnpj/Cpf</th>\r\n      <th scope=\"col\">Cep</th>\r\n      <th scope=\"col\">E-mail</th>\r\n      <th scope=\"col\">Rg</th>\r\n      <th scope=\"col\">Data de Nascimento</th>\r\n      <th scope=\"col\">Ação</th>\r\n    </tr>\r\n  </thead>\r\n  <tbody>\r\n    <tr  v-for=\"supplier in supplierFilter\"\r\n          v-bind:key=\"supplier.companyId\">\r\n      <td> {{ supplier.name }}</td>\r\n      <td> {{ supplier.cnpjCpf }}</td>\r\n      <td> {{supplier.cep}}</td>\r\n      <td> {{supplier.email}}</td>\r\n      <td> {{supplier.rg}}</td>\r\n      <td> {{supplier.birthday}}</td>\r\n      <td>  <button\r\n              class=\"delete-btn\"\r\n              @click=\"deleteCompany(supplier.suppliersId)\"\r\n            >\r\n              Deletar\r\n            </button></td>\r\n    </tr>\r\n    \r\n  </tbody>\r\n</table>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from \"axios\";\r\n  import Message from \"./Message\";\r\n  \r\n  export default {\r\n    name: \"SupplierTable\",\r\n  \r\n    data() {\r\n      return {\r\n        suppliers: [],\r\n        supplierFilter: [],\r\n        msg: \"\",\r\n      };\r\n    },\r\n    computed: {\r\n    filteredItems() {\r\n      let items = [];\r\n      items = this.items.filter((item) => {\r\n        return (\r\n          item.name.toLowerCase().indexOf(this.search.toLowerCase()) > -1 ||\r\n          item.cnpj.toLowerCase().indexOf(this.search.toLowerCase()) > -1\r\n        );\r\n      });\r\n\r\n      return items;\r\n    },\r\n    const: supplierFilter = computed(()=>{\r\n  if(suppliers.value && SearchName.value){\r\n    return suppliers.value.filter(supplier=>\r\n      supplier.name.toLowerCase().includes(SearchName.value.toLowerCase())\r\n    )\r\n  }\r\n  return suppliers.value;\r\n})\r\n  },\r\n    methods: {\r\n      getSuppliers() {\r\n        axios\r\n          .get(\"https://localhost:7237/api/suppliers/GetAll\")\r\n          .then((res) => {\r\n            this.suppliers = res.data;\r\n            this.supplierFilter = res.data;\r\n          })\r\n          .catch(function (error) {\r\n            console.error(error);\r\n          });\r\n      },\r\n      deleteCompany(id) {\r\n        if (confirm(\"Desaja deletar essa empresa?\")) {\r\n          axios\r\n            .delete(`https://localhost:7237/api/suppliers/Delete/${id}`)\r\n            .then((res) => {\r\n              this.suppliers = res.data;\r\n              this.supplierFilter = res.data;\r\n              this.getSuppliers();\r\n              this.msg = \"Empresa Deletada\";\r\n              setTimeout(() => (this.msg = \"\"), 2000);\r\n            })\r\n            .catch((error) => {\r\n              console.log(error);\r\n            });\r\n        }\r\n      },\r\n    },\r\n    created() {\r\n      this.getSuppliers();\r\n    },\r\n    components: {\r\n      Message,\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  input {\r\n  padding: 5px 10px;\r\n  width: 300px;\r\n  border-radius: 10px;\r\n  font-size: 16px;\r\n  margin-top: 7px;\r\n  border-color: #987bad;\r\n}\r\n  .delete-btn {\r\n  background-color: #222;\r\n  color           : #987bad;\r\n  font-weight     : bold;\r\n  border          : 2px solid #222;\r\n  padding         : 10px;\r\n  font-size       : 16px;\r\n  margin          : 0 auto;\r\n  cursor          : pointer;\r\n  transition      : 0.5s;\r\n  }\r\n  \r\n  .delete-btn:hover {\r\n    background-color: transparent;\r\n    color: #222;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAuCE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,M