{"ast":null,"code":"import axios from \"axios\";\nimport Message from \"./Message\";\nexport default {\n  name: \"CompanyTable\",\n  data() {\n    return {\n      companies: [],\n      msg: \"\"\n    };\n  },\n  methods: {\n    getCompanies() {\n      axios.get(\"https://localhost:7237/api/companies/Get\").then(res => {\n        console.log(res);\n        this.companies = res.data;\n      }).catch(function (error) {\n        console.error(error);\n      });\n    },\n    deleteCompany(id) {\n      if (confirm(\"Desaja deletar essa empresa?\")) {\n        axios.delete(`https://localhost:7237/api/companies/Delete/${id}`).then(res => {\n          this.companies = res.data;\n          this.getCompanies();\n          this.msg = \"Empresa Deletada\";\n          setTimeout(() => this.msg = \"\", 2000);\n        }).catch(error => {\n          console.log(error);\n        });\n      }\n    },\n    editCompany(company) {\n      this.company.name = document.getElementById(\"name\").value = company.name;\n      this.company.cnpj = document.getElementById(\"cnpj\").value = company.cnpj;\n      this.company.cep = document.getElementById(\"cep\").value = company.cep;\n      this.company = company;\n    },\n    alterCompanies() {\n      this.company.name = document.getElementById(\"name\").value;\n      this.company.cnpj = document.getElementById(\"cnpj\").value;\n      this.company.cep = document.getElementById(\"cep\").value;\n      axios.put(`https://localhost:7237/api/companies/Update/${this.company.id}`, this.company).then(() => {\n        this.getCompanies();\n        this.company = undefined;\n        document.getElementById(\"name\").value = \"\";\n        document.getElementById(\"cnpj\").value = \"\";\n        document.getElementById(\"cep\").value = \"\";\n      });\n    }\n  },\n  created() {\n    this.getCompanies();\n  },\n  components: {\n    Message\n  }\n};","map":{"version":3,"names":["axios","Message","name","data","companies","msg","methods","getCompanies","get","then","res","console","log","catch","error","deleteCompany","id","confirm","delete","setTimeout","editCompany","company","document","getElementById","value","cnpj","cep","alterCompanies","put","undefined","created","components"],"sources":["C:\\Users\\Thata\\OneDrive\\Área de Trabalho\\DesafioAccenture\\desafiofullstack-app\\src\\components\\CompanyTable.vue"],"sourcesContent":["<template>\r\n  <div id=\"company-table\">\r\n    <Message :msg=\"msg\" v-show=\"msg\" />\r\n    <div id=\"company-table-heading\">\r\n      <div>Nome:</div>\r\n      <div>Cnpj:</div>\r\n      <div>Cep:</div>\r\n      <div>Ações</div>\r\n    </div>\r\n    <div id=\"company-table-rows\">\r\n      <div\r\n        class=\"company-table-row\"\r\n        v-for=\"company in companies\"\r\n        :key=\"company.companiesId\"\r\n      >\r\n        <div>\r\n          {{ company.name }}\r\n        </div>\r\n        <div>\r\n          {{ company.cnpj }}\r\n        </div>\r\n        <div>\r\n          {{ company.cep }}\r\n        </div>\r\n        <div>\r\n          <button class=\"edit-btn\" @click=\"editCompany(company)\">\r\n            Editar\r\n          </button>\r\n          <button\r\n            class=\"delete-btn\"\r\n            @click=\"deleteCompany(company.companiesId)\"\r\n          >\r\n            Deletar\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport Message from \"./Message\";\r\n\r\nexport default {\r\n  name: \"CompanyTable\",\r\n\r\n  data() {\r\n    return {\r\n      companies: [],\r\n      msg: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    getCompanies() {\r\n      axios\r\n        .get(\"https://localhost:7237/api/companies/Get\")\r\n        .then((res) => {\r\n          console.log(res);\r\n          this.companies = res.data;\r\n        })\r\n        .catch(function (error) {\r\n          console.error(error);\r\n        });\r\n    },\r\n    deleteCompany(id) {\r\n      if (confirm(\"Desaja deletar essa empresa?\")) {\r\n        axios\r\n          .delete(`https://localhost:7237/api/companies/Delete/${id}`)\r\n          .then((res) => {\r\n            this.companies = res.data;\r\n            this.getCompanies();\r\n            this.msg = \"Empresa Deletada\";\r\n            setTimeout(() => (this.msg = \"\"), 2000);\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          });\r\n      }\r\n    },\r\n    editCompany(company) {\r\n     this.company.name = document.getElementById(\"name\").value = company.name\r\n     this.company.cnpj = document.getElementById(\"cnpj\").value = company.cnpj\r\n     this.company.cep = document.getElementById(\"cep\").value = company.cep\r\n     this.company = company\r\n    },\r\n    alterCompanies() {\r\n     this.company.name = document.getElementById(\"name\").value\r\n     this.company.cnpj = document.getElementById(\"cnpj\").value\r\n     this.company.cep = document.getElementById(\"cep\").value\r\n\r\n     axios.put(`https://localhost:7237/api/companies/Update/${this.company.id}`, this.company).then(() =>\r\n     {\r\n      this.getCompanies()\r\n      this.company = undefined\r\n      document.getElementById(\"name\").value = \"\"\r\n      document.getElementById(\"cnpj\").value = \"\"\r\n      document.getElementById(\"cep\").value = \"\"\r\n\r\n     })\r\n\r\n\r\n    },\r\n  },\r\n  created() {\r\n    this.getCompanies();\r\n  },\r\n  components: {\r\n    Message,\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#company-table {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\n#company-table-heading,\r\n#company-table-rows,\r\n.company-table-row {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n#company-table-heading {\r\n  font-weight: bold;\r\n  padding: 12px;\r\n  border-bottom: 3px solid #333;\r\n}\r\n\r\n.company-table-row {\r\n  width: 100%;\r\n  padding: 12px;\r\n  border-bottom: 1px solid #ccc;\r\n}\r\n\r\n#company-table-heading div,\r\n.company-table-row div {\r\n  width: 19%;\r\n}\r\n\r\n#company-table-heading .order-id,\r\n.company-table-row .order-number {\r\n  width: 5%;\r\n}\r\n\r\n.edit-btn {\r\n  background-color: #222;\r\n  color: #fcba03;\r\n  font-weight: bold;\r\n  border: 2px solid #222;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  margin: 0 auto;\r\n  cursor: pointer;\r\n  transition: 0.5s;\r\n}\r\n\r\n.edit-btn:hover {\r\n  background-color: transparent;\r\n  color: #222;\r\n}\r\n.delete-btn {\r\n  background-color: #222;\r\n  color: #fcba03;\r\n  font-weight: bold;\r\n  border: 2px solid #222;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  margin: 0 auto;\r\n  cursor: pointer;\r\n  transition: 0.5s;\r\n}\r\n\r\n.delete-btn:hover {\r\n  background-color: transparent;\r\n  color: #222;\r\n}\r\n</style>\r\n"],"mappings":"AAyCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,WAAW;AAE/B,eAAe;EACbC,IAAI,EAAE,cAAc;EAEpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,GAAG,EAAE;IACP,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACbP,KAAI,CACDQ,GAAG,CAAC,0CAA0C,EAC9CC,IAAI,CAAEC,GAAG,IAAK;QACbC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;QAChB,IAAI,CAACN,SAAQ,GAAIM,GAAG,CAACP,IAAI;MAC3B,CAAC,EACAU,KAAK,CAAC,UAAUC,KAAK,EAAE;QACtBH,OAAO,CAACG,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDC,aAAaA,CAACC,EAAE,EAAE;MAChB,IAAIC,OAAO,CAAC,8BAA8B,CAAC,EAAE;QAC3CjB,KAAI,CACDkB,MAAM,CAAE,+CAA8CF,EAAG,EAAC,EAC1DP,IAAI,CAAEC,GAAG,IAAK;UACb,IAAI,CAACN,SAAQ,GAAIM,GAAG,CAACP,IAAI;UACzB,IAAI,CAACI,YAAY,CAAC,CAAC;UACnB,IAAI,CAACF,GAAE,GAAI,kBAAkB;UAC7Bc,UAAU,CAAC,MAAO,IAAI,CAACd,GAAE,GAAI,EAAG,EAAE,IAAI,CAAC;QACzC,CAAC,EACAQ,KAAK,CAAEC,KAAK,IAAK;UAChBH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;QACpB,CAAC,CAAC;MACN;IACF,CAAC;IACDM,WAAWA,CAACC,OAAO,EAAE;MACpB,IAAI,CAACA,OAAO,CAACnB,IAAG,GAAIoB,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,KAAI,GAAIH,OAAO,CAACnB,IAAG;MACvE,IAAI,CAACmB,OAAO,CAACI,IAAG,GAAIH,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,KAAI,GAAIH,OAAO,CAACI,IAAG;MACvE,IAAI,CAACJ,OAAO,CAACK,GAAE,GAAIJ,QAAQ,