n        <div>\r\n          <button class=\"edit-btn\" @click=\"updatecompany(company.id)\">\r\n            Editar\r\n          </button>\r\n          <button class=\"delete-btn\" @click=\"deletecompany(company.id)\">\r\n            Deletar\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div v-else>\r\n    <h2>Não há pedidos no momento!</h2>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"Dashboard\",\r\n  data() {\r\n    return {\r\n      companys: [],\r\n    };\r\n  },\r\n  methods: {\r\n    getCompanies() {\r\n      axios\r\n        .get(\"https://localhost:7237/api/companies/Get\")\r\n        .then((res) => {\r\n          console.log(res);\r\n          this.companies = res.data;\r\n        })\r\n        .catch(function (error) {\r\n          console.error(error);\r\n        });\r\n    },\r\n    async updatecompany(event, id) {\r\n      const option = event.target.value;\r\n\r\n      const dataJson = JSON.stringify({ status: option });\r\n\r\n      const req = await fetch(`https://localhost:7237/api/companies/${id}`, {\r\n        method: \"PATCH\",\r\n        headers: { \"Content-Type\": \"application/json\" },\r\n        body: dataJson,\r\n      });\r\n\r\n      const res = await req.json();\r\n\r\n      console.log(res);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getPedidos();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#company-table {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\n#company-table-heading,\r\n#company-table-rows,\r\n.company-table-row {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n#company-table-heading {\r\n  font-weight: bold;\r\n  padding: 12px;\r\n  border-bottom: 3px solid #333;\r\n}\r\n\r\n.company-table-row {\r\n  width: 100%;\r\n  padding: 12px;\r\n  border-bottom: 1px solid #ccc;\r\n}\r\n\r\n#company-table-heading div,\r\n.company-table-row div {\r\n  width: 19%;\r\n}\r\n\r\n#company-table-heading .order-id,\r\n.company-table-row .order-number {\r\n  width: 5%;\r\n}\r\n\r\nselect {\r\n  padding: 12px 6px;\r\n  margin-right: 12px;\r\n}\r\n\r\n.delete-btn {\r\n  background-color: #222;\r\n  color: #fcba03;\r\n  font-weight: bold;\r\n  border: 2px solid #222;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  margin: 0 auto;\r\n  cursor: pointer;\r\n  transition: 0.5s;\r\n}\r\n\r\n.delete-btn:hover {\r\n  background-color: transparent;\r\n  color: #222;\r\n}\r\n\r\n.edit-btn {\r\n  background-color: #222;\r\n  color: #fcba03;\r\n  font-weight: bold;\r\n  border: 2px solid #222;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  margin: 0 auto;\r\n  cursor: pointer;\r\n  transition: 0.5s;\r\n}\r\n\r\n.edit-btn:hover {\r\n  background-color: transparent;\r\n  color: #222;\r\n}\r\n</style>\r\n"],"mappings":"AAoCA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACbC,KAAI,CACDC,GAAG,CAAC,0CAA0C,EAC9CC,IAAI,CAAEC,GAAG,IAAK;QACbC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;QAChB,IAAI,CAACG,SAAQ,GAAIH,GAAG,CAACP,IAAI;MAC3B,CAAC,EACAW,KAAK,CAAC,UAAUC,KAAK,EAAE;QACtBJ,OAAO,CAACI,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACD,MAAMC,aAAaA,CAACC,KAAK,EAAEC,EAAE,EAAE;MAC7B,MAAMC,MAAK,GAAIF,KAAK,CAACG,MAAM,CAACC,KAAK;MAEjC,MAAMC,QAAO,GAAIC,IAAI,CAACC,SAAS,CAAC;QAAEC,MAAM,EAAEN;MAAO,CAAC,CAAC;MAEnD,MAAMO,GAAE,GAAI,MAAMC,KAAK,CAAE,wCAAuCT,EAAG,EAAC,EAAE;QACpEU,MAAM,EAAE,OAAO;QACfC,OAAO,EAAE;UAAE,cAAc,EAAE;QAAmB,CAAC;QAC/CC,IAAI,EAAER;MACR,CAAC,CAAC;MAEF,MAAMZ,GAAE,GAAI,MAAMgB,GAAG,CAACK,IAAI,CAAC,CAAC;MAE5BpB,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IAClB;EACF,CAAC;EACDsB,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}